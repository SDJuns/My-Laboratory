import java.util.Scanner;
public class practiceWithMy {
public static void main(String[] args) {
	
		Scanner s = new Scanner(System.in);
		
		int intArray[] = new int[5]; // Create the 5th array. 
		int sum = 0;
		System.out.println("Please insert the figures in this here.>>");
		for(int i = 0; i < 5; i++) {
			intArray[i] = s.nextInt();
			sum += intArray[i];
			//if (intArray[i] == intArray.length)
			//	System.out.print("Sum's average is " + (double)sum/i + ".");
			if (i == 4)
				System.out.print("Sum's average is " + (double)sum/i + ".");
			//count++;
			//if(count == 0 )
			//	System.out.print("None.");
			//else {
			//	System.out.print("Sum is " + sum + ".");
			//	System.out.print("Sum's average is " + (double)sum/count + ".");
			//}
		}
		s.close();
}
}
