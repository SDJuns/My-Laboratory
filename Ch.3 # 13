public class practiceWithMy13 {
public static void main(String[] args) {
	int a;
	for(int i = 1; i< 100; i++) {
		System.out.println(i);
		a = 0;
		if((i % 10 == 3) || (i % 10 == 6) || (i % 10 == 9))
			a++;
		if((i / 10 == 3)|| (i / 10 == 6) || (i / 10 == 9))
			a++;
		
		if(a == 1)
			System.out.println(i + " hand clap!");
		else if(a == 2)
			System.out.println(i +" hand clap clap!");
	}
}
}
